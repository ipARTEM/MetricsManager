@model TestMSSQL.Models.CpuMetric
@{
    ViewBag.Title = "Добавление пользователя";
}
<form asp-action="create" asp-controller="CpuMetric">
    <div class="form-group">
        <label asp-for="Id" class="control-label">Id</label>
        <input type="text" asp-for="Time" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Value" class="control-label">Value</label>
        <input type="number" asp-for="Value" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Отправить" class="btn btn-default" />
    </div>
</form>
